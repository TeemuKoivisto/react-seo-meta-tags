{"version":3,"file":"get-cache.js","names":["caches","Map","getCache","name","cache","get","GatsbyCacheLmdb","require","default","init","set"],"sources":["../../src/utils/get-cache.ts"],"sourcesContent":["import GatsbyCache from \"./cache\"\n\nconst caches = new Map<string, GatsbyCache>()\n\nexport const getCache = (name: string): GatsbyCache => {\n  let cache = caches.get(name)\n  if (!cache) {\n    const GatsbyCacheLmdb = require(`./cache-lmdb`).default\n    cache = new GatsbyCacheLmdb({ name }).init() as GatsbyCache\n    caches.set(name, cache)\n  }\n  return cache\n}\n"],"mappings":";;;;AAEA,MAAMA,MAAM,GAAG,IAAIC,GAAG,EAAuB;AAEtC,MAAMC,QAAQ,GAAIC,IAAY,IAAkB;EACrD,IAAIC,KAAK,GAAGJ,MAAM,CAACK,GAAG,CAACF,IAAI,CAAC;EAC5B,IAAI,CAACC,KAAK,EAAE;IACV,MAAME,eAAe,GAAGC,OAAO,CAAE,cAAa,CAAC,CAACC,OAAO;IACvDJ,KAAK,GAAG,IAAIE,eAAe,CAAC;MAAEH;IAAK,CAAC,CAAC,CAACM,IAAI,EAAiB;IAC3DT,MAAM,CAACU,GAAG,CAACP,IAAI,EAAEC,KAAK,CAAC;EACzB;EACA,OAAOA,KAAK;AACd,CAAC;AAAA"}