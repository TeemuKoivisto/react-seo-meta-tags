(): Array<DocumentNode> =>\n  allSdlTypes.map(type => parse(type))\n\nexport const builtInScalarTypeNames = [\n  `Boolean`,\n  `Date`,\n  `Float`,\n  `ID`,\n  `Int`,\n  `JSON`,\n  `String`,\n  `GatsbyImageData`,\n]\n\nexport const internalTypeNames = [\n  ...builtInScalarTypeNames,\n  `Buffer`,\n  `Internal`,\n  `InternalInput`,\n  `Node`,\n  `NodeInput`,\n  `Query`,\n]\n"],"mappings":";;;;AAAA;AAEA;;AAEA,MAAMA,QAAQ,GAAI;AAClB;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,CAAC;AAED,MAAMC,gBAAgB,GAAI;AAC1B;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,CAAC;AAED,MAAMC,aAAa,GAAI;AACvB;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,CAAC;AAED,MAAMC,QAAQ,GAAI;AAClB;AACA;AACA;AACA;AACA,CAAC;AAED,MAAMC,oBAAoB,GAAI;AAC9B;AACA;AACA;AACA;AACA,CAAC;AAED,MAAMC,YAAY,GAAI;AACtB;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,CAAC;AAED,MAAMC,cAAc,GAAI;AACxB;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,CAAC;AAED,MAAMC,qBAAqB,GAAI;AAC/B;AACA;AACA;AACA,CAAC;AAED,MAAMC,WAAW,GAAG,CAClBR,QAAQ,EACRE,aAAa,EACbC,QAAQ,EACRC,oBAAoB,EACpBH,gBAAgB,EAChBI,YAAY,EACZC,cAAc,EACdC,qBAAqB,CACtB;AAEM,MAAME,2BAA2B,GAAG,IAAIC,GAAG,CAAC,CAAE,kBAAiB,CAAC,CAAC;AAAA;AAEjE,MAAMC,sBAAsB,GAAG,MACpCH,WAAW,CAACI,GAAG,CAACC,IAAI,IAAI,IAAAC,cAAK,EAACD,IAAI,CAAC,CAAC;AAAA;AAE/B,MAAME,sBAAsB,GAAG,CACnC,SAAQ,EACR,MAAK,EACL,OAAM,EACN,IAAG,EACH,KAAI,EACJ,MAAK,EACL,QAAO,EACP,iBAAgB,CAClB;AAAA;AAEM,MAAMC,iBAAiB,GAAG,CAC/B,GAAGD,sBAAsB,EACxB,QAAO,EACP,UAAS,EACT,eAAc,EACd,MAAK,EACL,WAAU,EACV,OAAM,CACR;AAAA"}