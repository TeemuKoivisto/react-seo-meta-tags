{"version":3,"file":"sample-site-for-experiment.js","names":["sampleSite","experimentName","percentage","bucketNumber","murmurhash","JSON","stringify","getRepositoryId","repositoryId"],"sources":["../../src/utils/sample-site-for-experiment.ts"],"sourcesContent":["import { getRepositoryId } from \"gatsby-telemetry/lib/repository-id\"\nimport { murmurhash } from \"gatsby-core-utils/murmurhash\"\n\nconst sampleSite = (experimentName: string, percentage: number): boolean => {\n  const bucketNumber =\n    murmurhash(\n      experimentName + `` + JSON.stringify(getRepositoryId().repositoryId),\n      0\n    ) % 100\n\n  return bucketNumber < percentage\n}\n\nexport default sampleSite\n"],"mappings":";;;;AAAA;AACA;AAEA,MAAMA,UAAU,GAAG,CAACC,cAAsB,EAAEC,UAAkB,KAAc;EAC1E,MAAMC,YAAY,GAChB,IAAAC,sBAAU,EACRH,cAAc,GAAI,EAAC,GAAGI,IAAI,CAACC,SAAS,CAAC,IAAAC,6BAAe,GAAE,CAACC,YAAY,CAAC,EACpE,CAAC,CACF,GAAG,GAAG;EAET,OAAOL,YAAY,GAAGD,UAAU;AAClC,CAAC;AAAA,eAEcF,UAAU;AAAA"}