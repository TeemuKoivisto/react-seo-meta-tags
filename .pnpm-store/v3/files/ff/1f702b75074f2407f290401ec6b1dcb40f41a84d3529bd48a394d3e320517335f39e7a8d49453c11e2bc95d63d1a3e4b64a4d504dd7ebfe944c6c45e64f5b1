{"version":3,"file":"nodes.js","names":["nodesReducer","state","Map","action","type","set","payload","id","delete"],"sources":["../../../src/redux/reducers/nodes.ts"],"sourcesContent":["import { ActionsUnion, IGatsbyState } from \"../types\"\n\nexport const nodesReducer = (\n  state: IGatsbyState[\"nodes\"] = new Map(),\n  action: ActionsUnion\n): IGatsbyState[\"nodes\"] => {\n  switch (action.type) {\n    case `DELETE_CACHE`:\n      return new Map()\n\n    case `CREATE_NODE`: {\n      state.set(action.payload.id, action.payload)\n      return state\n    }\n\n    case `ADD_FIELD_TO_NODE`:\n    case `ADD_CHILD_NODE_TO_PARENT_NODE`:\n      state.set(action.payload.id, action.payload)\n      return state\n\n    case `DELETE_NODE`: {\n      if (action.payload) {\n        state.delete(action.payload.id)\n      }\n      return state\n    }\n\n    default:\n      return state\n  }\n}\n"],"mappings":";;;;AAEO,MAAMA,YAAY,GAAG,CAC1BC,KAA4B,GAAG,IAAIC,GAAG,EAAE,EACxCC,MAAoB,KACM;EAC1B,QAAQA,MAAM,CAACC,IAAI;IACjB,KAAM,cAAa;MACjB,OAAO,IAAIF,GAAG,EAAE;IAElB,KAAM,aAAY;MAAE;QAClBD,KAAK,CAACI,GAAG,CAACF,MAAM,CAACG,OAAO,CAACC,EAAE,EAAEJ,MAAM,CAACG,OAAO,CAAC;QAC5C,OAAOL,KAAK;MACd;IAEA,KAAM,mBAAkB;IACxB,KAAM,+BAA8B;MAClCA,KAAK,CAACI,GAAG,CAACF,MAAM,CAACG,OAAO,CAACC,EAAE,EAAEJ,MAAM,CAACG,OAAO,CAAC;MAC5C,OAAOL,KAAK;IAEd,KAAM,aAAY;MAAE;QAClB,IAAIE,MAAM,CAACG,OAAO,EAAE;UAClBL,KAAK,CAACO,MAAM,CAACL,MAAM,CAACG,OAAO,CAACC,EAAE,CAAC;QACjC;QACA,OAAON,KAAK;MACd;IAEA;MACE,OAAOA,KAAK;EAAA;AAElB,CAAC;AAAA"}