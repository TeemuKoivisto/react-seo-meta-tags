{"version":3,"file":"slices.js","names":["slicesReducer","state","Map","action","type","set","payload","name","delete"],"sources":["../../../src/redux/reducers/slices.ts"],"sourcesContent":["import { IGatsbyState, ActionsUnion, IGatsbySlice } from \"../types\"\n\nexport const slicesReducer = (\n  state: IGatsbyState[\"slices\"] = new Map<string, IGatsbySlice>(),\n  action: ActionsUnion\n): IGatsbyState[\"slices\"] => {\n  switch (action.type) {\n    case `CREATE_SLICE`: {\n      state.set(action.payload.name, action.payload)\n      return state\n    }\n\n    case `DELETE_SLICE`: {\n      state.delete(action.payload.name)\n      return state\n    }\n\n    default:\n      return state\n  }\n}\n"],"mappings":";;;;AAEO,MAAMA,aAAa,GAAG,CAC3BC,KAA6B,GAAG,IAAIC,GAAG,EAAwB,EAC/DC,MAAoB,KACO;EAC3B,QAAQA,MAAM,CAACC,IAAI;IACjB,KAAM,cAAa;MAAE;QACnBH,KAAK,CAACI,GAAG,CAACF,MAAM,CAACG,OAAO,CAACC,IAAI,EAAEJ,MAAM,CAACG,OAAO,CAAC;QAC9C,OAAOL,KAAK;MACd;IAEA,KAAM,cAAa;MAAE;QACnBA,KAAK,CAACO,MAAM,CAACL,MAAM,CAACG,OAAO,CAACC,IAAI,CAAC;QACjC,OAAON,KAAK;MACd;IAEA;MACE,OAAOA,KAAK;EAAA;AAElB,CAAC;AAAA"}