`)\n  console.log(\n    `To create a production build, use ` + `${chalk.cyan(`gatsby build`)}`\n  )\n  console.log()\n}\n"],"mappings":";;;;;AAAA;AAIO,SAASA,iBAAiB,CAACC,OAAe,EAAEC,IAAmB,EAAQ;EAC5EC,OAAO,CAACC,GAAG,EAAE;EACbD,OAAO,CAACC,GAAG,CAAE,oBAAmBC,cAAK,CAACC,IAAI,CAACL,OAAO,CAAE,kBAAiB,CAAC;EACtEE,OAAO,CAACC,GAAG,EAAE;EAEb,IAAIF,IAAI,CAACK,iBAAiB,EAAE;IAC1BJ,OAAO,CAACC,GAAG,CACR,KAAIC,cAAK,CAACC,IAAI,CAAE,QAAO,CAAE,eAAcJ,IAAI,CAACM,mBAAoB,EAAC,CACnE;IACDL,OAAO,CAACC,GAAG,CACR,KAAIC,cAAK,CAACC,IAAI,CAAE,kBAAiB,CAAE,KAAIJ,IAAI,CAACK,iBAAkB,EAAC,CACjE;EACH,CAAC,MAAM;IACLJ,OAAO,CAACC,GAAG,CAAE,KAAIF,IAAI,CAACM,mBAAoB,EAAC,CAAC;EAC9C;EAEAL,OAAO,CAACC,GAAG,EAAE;EACbD,OAAO,CAACC,GAAG,CACR,0EAAyE,CAC3E;EACDD,OAAO,CAACC,GAAG,EAAE;EAEb,IAAIF,IAAI,CAACK,iBAAiB,EAAE;IAC1BJ,OAAO,CAACC,GAAG,CACR,KAAIC,cAAK,CAACC,IAAI,CAAE,QAAO,CAAE,eACxBJ,IAAI,CAACM,mBACN,YAAW,CACb;IACDL,OAAO,CAACC,GAAG,CACR,KAAIC,cAAK,CAACC,IAAI,CAAE,kBAAiB,CAAE,KAAIJ,IAAI,CAACK,iBAAkB,YAAW,CAC3E;EACH,CAAC,MAAM;IACLJ,OAAO,CAACC,GAAG,CAAE,KAAIF,IAAI,CAACM,mBAAoB,YAAW,CAAC;EACxD;EAEA,IAAIC,OAAO,CAACC,GAAG,CAACC,gCAAgC,EAAE;IAChDR,OAAO,CAACC,GAAG,EAAE;IACbD,OAAO,CAACC,GAAG,CACR,mEAAkE,CACpE;IACDD,OAAO,CAACC,GAAG,EAAE;IAEb,IAAIF,IAAI,CAACK,iBAAiB,EAAE;MAC1BJ,OAAO,CAACC,GAAG,CACR,KAAIC,cAAK,CAACC,IAAI,CAAE,QAAO,CAAE,eACxBJ,IAAI,CAACM,mBACN,UAAS,CACX;MACDL,OAAO,CAACC,GAAG,CACR,KAAIC,cAAK,CAACC,IAAI,CAAE,kBAAiB,CAAE,KAAIJ,IAAI,CAACK,iBAAkB,UAAS,CACzE;IACH,CAAC,MAAM;MACLJ,OAAO,CAACC,GAAG,CAAE,KAAIF,IAAI,CAACM,mBAAoB,UAAS,CAAC;IACtD;EACF;EAEAL,OAAO,CAACC,GAAG,EAAE;EACbD,OAAO,CAACC,GAAG,CAAE,mDAAkD,CAAC;EAChED,OAAO,CAACC,GAAG,CACR,oCAAmC,GAAI,GAAEC,cAAK,CAACO,IAAI,CAAE,cAAa,CAAE,EAAC,CACvE;EACDT,OAAO,CAACC,GAAG,EAAE;AACf"}