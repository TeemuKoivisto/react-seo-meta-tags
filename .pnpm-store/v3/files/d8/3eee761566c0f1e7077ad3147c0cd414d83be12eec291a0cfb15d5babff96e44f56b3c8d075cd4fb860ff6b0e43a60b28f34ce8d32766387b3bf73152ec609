{"version":3,"file":"set-request-headers.js","names":["setRequestHeadersReducer","state","Map","action","type","headers","domain","payload","set"],"sources":["../../../src/redux/reducers/set-request-headers.ts"],"sourcesContent":["import { IGatsbyState, ISetDomainRequestHeaders } from \"../types\"\n\n/**\n * Takes in a domain and headers for that domain, from the setRequestHeaders action, and stores them in a Map to be accessed when making requests.\n */\nexport const setRequestHeadersReducer = (\n  state: IGatsbyState[\"requestHeaders\"] = new Map(),\n  action: ISetDomainRequestHeaders\n): IGatsbyState[\"requestHeaders\"] => {\n  switch (action.type) {\n    case `SET_REQUEST_HEADERS`: {\n      const { headers, domain } = action.payload\n\n      state.set(domain, headers)\n      return state\n    }\n    default:\n      return state\n  }\n}\n"],"mappings":";;;;AAEA;AACA;AACA;AACO,MAAMA,wBAAwB,GAAG,CACtCC,KAAqC,GAAG,IAAIC,GAAG,EAAE,EACjDC,MAAgC,KACG;EACnC,QAAQA,MAAM,CAACC,IAAI;IACjB,KAAM,qBAAoB;MAAE;QAC1B,MAAM;UAAEC,OAAO;UAAEC;QAAO,CAAC,GAAGH,MAAM,CAACI,OAAO;QAE1CN,KAAK,CAACO,GAAG,CAACF,MAAM,EAAED,OAAO,CAAC;QAC1B,OAAOJ,KAAK;MACd;IACA;MACE,OAAOA,KAAK;EAAA;AAElB,CAAC;AAAA"}