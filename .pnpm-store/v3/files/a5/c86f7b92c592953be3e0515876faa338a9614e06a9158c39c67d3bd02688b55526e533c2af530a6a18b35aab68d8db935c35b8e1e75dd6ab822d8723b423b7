{"version":3,"file":"server-slice-renderer.js","names":["ServerSliceRenderer","sliceId","children","contents","React","createElement","id","push"],"sources":["../../slice/server-slice-renderer.js"],"sourcesContent":["import React from \"react\"\n\nexport const ServerSliceRenderer = ({ sliceId, children }) => {\n  const contents = [\n    React.createElement(`slice-start`, {\n      id: `${sliceId}-1`,\n    }),\n    React.createElement(`slice-end`, {\n      id: `${sliceId}-1`,\n    }),\n  ]\n\n  if (children) {\n    // if children exist, we split the slice into a before and after piece\n    // see renderSlices in render-html\n    contents.push(children)\n    contents.push(\n      React.createElement(`slice-start`, {\n        id: `${sliceId}-2`,\n      }),\n      React.createElement(`slice-end`, {\n        id: `${sliceId}-2`,\n      })\n    )\n  }\n\n  return contents\n}\n"],"mappings":";;;;;AAAA;AAEO,MAAMA,mBAAmB,GAAG,CAAC;EAAEC,OAAO;EAAEC;AAAS,CAAC,KAAK;EAC5D,MAAMC,QAAQ,GAAG,cACfC,cAAK,CAACC,aAAa,CAAE,aAAY,EAAE;IACjCC,EAAE,EAAG,GAAEL,OAAQ;EACjB,CAAC,CAAC,eACFG,cAAK,CAACC,aAAa,CAAE,WAAU,EAAE;IAC/BC,EAAE,EAAG,GAAEL,OAAQ;EACjB,CAAC,CAAC,CACH;EAED,IAAIC,QAAQ,EAAE;IACZ;IACA;IACAC,QAAQ,CAACI,IAAI,CAACL,QAAQ,CAAC;IACvBC,QAAQ,CAACI,IAAI,eACXH,cAAK,CAACC,aAAa,CAAE,aAAY,EAAE;MACjCC,EAAE,EAAG,GAAEL,OAAQ;IACjB,CAAC,CAAC,eACFG,cAAK,CAACC,aAAa,CAAE,WAAU,EAAE;MAC/BC,EAAE,EAAG,GAAEL,OAAQ;IACjB,CAAC,CAAC,CACH;EACH;EAEA,OAAOE,QAAQ;AACjB,CAAC;AAAA"}