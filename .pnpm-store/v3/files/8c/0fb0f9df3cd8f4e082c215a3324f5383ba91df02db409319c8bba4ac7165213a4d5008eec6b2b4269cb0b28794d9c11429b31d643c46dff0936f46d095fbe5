{"version":3,"file":"get-value-at.js","names":["getValueAt","obj","selector","selectors","split","get","undefined","Array","isArray","getArray","key","rest","value","length","arr","map","filter","v"],"sources":["../../src/utils/get-value-at.ts"],"sourcesContent":["export function getValueAt(\n  obj: Record<string, unknown>,\n  selector: string | Array<string>\n): any {\n  const selectors =\n    typeof selector === `string` ? selector.split(`.`) : selector\n  return get(obj, selectors)\n}\n\nfunction get(obj: unknown, selectors: Array<string>): any {\n  if (typeof obj !== `object` || obj === null) return undefined\n  if (Array.isArray(obj)) return getArray(obj, selectors)\n  const [key, ...rest] = selectors\n  const value = obj[key]\n  if (!rest.length) return value\n  if (Array.isArray(value)) return getArray(value, rest)\n  if (value && typeof value === `object`) return get(value, rest)\n  return undefined\n}\n\nfunction getArray(arr: Array<unknown>, selectors: Array<string>): Array<any> {\n  return arr\n    .map(value =>\n      Array.isArray(value) ? getArray(value, selectors) : get(value, selectors)\n    )\n    .filter(v => v !== undefined)\n}\n"],"mappings":";;;;AAAO,SAASA,UAAU,CACxBC,GAA4B,EAC5BC,QAAgC,EAC3B;EACL,MAAMC,SAAS,GACb,OAAOD,QAAQ,KAAM,QAAO,GAAGA,QAAQ,CAACE,KAAK,CAAE,GAAE,CAAC,GAAGF,QAAQ;EAC/D,OAAOG,GAAG,CAACJ,GAAG,EAAEE,SAAS,CAAC;AAC5B;AAEA,SAASE,GAAG,CAACJ,GAAY,EAAEE,SAAwB,EAAO;EACxD,IAAI,OAAOF,GAAG,KAAM,QAAO,IAAIA,GAAG,KAAK,IAAI,EAAE,OAAOK,SAAS;EAC7D,IAAIC,KAAK,CAACC,OAAO,CAACP,GAAG,CAAC,EAAE,OAAOQ,QAAQ,CAACR,GAAG,EAAEE,SAAS,CAAC;EACvD,MAAM,CAACO,GAAG,EAAE,GAAGC,IAAI,CAAC,GAAGR,SAAS;EAChC,MAAMS,KAAK,GAAGX,GAAG,CAACS,GAAG,CAAC;EACtB,IAAI,CAACC,IAAI,CAACE,MAAM,EAAE,OAAOD,KAAK;EAC9B,IAAIL,KAAK,CAACC,OAAO,CAACI,KAAK,CAAC,EAAE,OAAOH,QAAQ,CAACG,KAAK,EAAED,IAAI,CAAC;EACtD,IAAIC,KAAK,IAAI,OAAOA,KAAK,KAAM,QAAO,EAAE,OAAOP,GAAG,CAACO,KAAK,EAAED,IAAI,CAAC;EAC/D,OAAOL,SAAS;AAClB;AAEA,SAASG,QAAQ,CAACK,GAAmB,EAAEX,SAAwB,EAAc;EAC3E,OAAOW,GAAG,CACPC,GAAG,CAACH,KAAK,